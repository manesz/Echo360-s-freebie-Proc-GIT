@{
    ViewBag.Title = "RenderResult";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

         @if (ViewBag.Type == 1) {
                @Html.Partial("../Statics/RegisterResultSuccess")
        }
        else {
            if (ViewBag.Type == 2)
            { 
                    @Html.Partial("../Statics/RegisterResultPending")
            }
            else {
                if (ViewBag.Type == 3)
                { 
                    @Html.Partial("../Statics/SendPasswordToEmail")
                }
                else {
                    if (ViewBag.Type == 4)
                    { 
                        @Html.Partial("../Statics/ChangePasswordResult")
                    }
                    else { 
                        if (ViewBag.Type == 5)
                    { 
                        @Html.Partial("../Statics/GetUsernameResult")
                    }
                    
                    }            
                }
            }
        }
 
